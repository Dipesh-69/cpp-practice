// wap that asks the user to input 5 number and print their sum
//SMA

#include<iostream>
using namespace std;
int main() 
{
    int a[5];
    cout<<"enter any five number"<<endl;
    for(int i=0; i,5; i++)
    {
        cin>>a[i];
    }
    int sum =0;
    for(int i=0; i<5; i++)
    {
        sum = sum +a[i];
    }
    cout<<"sum="<<sum<<endl;
    return 0;


}